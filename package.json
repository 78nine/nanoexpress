{
  "name": "nanoexpress-pro",
  "version": "1.10.4",
  "description": "Nano-framework for Node.js powered by uWebSockets.js",
  "main": "cjs/nanoexpress.cjs",
  "module": "src/nanoexpress.js",
  "typings": "nanoexpress.d.ts",
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/dalisoft"
  },
  "scripts": {
    "lint": "eslint ./src",
    "test": "jest --config jest.config.js",
    "coverage": "npm run test --coverage",
    "coveralls": "npm run coverage && cat ./tests/coverage/lcov.info | coveralls",
    "prepublishOnly": "npm run lint && npm run build",
    "build": "rm -rf cjs && npm run test && rollup -c"
  },
  "exports": {
    ".": "./src/nanoexpress.js",
    "./Route": "./src/Route.js",
    "./static": "./src/static.js",
    "./defines": "./src/packed/defines/index.js",
    "./middlewares": "./src/packed/middlewares/index.js"
  },
  "engines": {
    "node": ">=12.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nanoexpress/pro.git"
  },
  "keywords": [
    "nano",
    "node",
    "fast",
    "uws",
    "websocket",
    "http",
    "c++"
  ],
  "author": "dalisoft",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/nanoexpress/pro/issues"
  },
  "homepage": "https://github.com/nanoexpress/pro#readme",
  "dependencies": {
    "@dalisoft/events": "^0.2.0",
    "ajv": "^6.11.0",
    "busboy": "^0.3.1",
    "cookie": "^0.x",
    "fast-json-stringify": "^1.16.3",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#binaries"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-jest": "^25.1.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "rollup": "^1.31.0"
  },
  "peerDependencies": {
    "passport": "^0.4.0",
    "swagger-ui-dist": "^3.23.6",
    "ws": "^7.2.0"
  }
}
